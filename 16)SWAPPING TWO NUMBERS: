//swapping number in c
#include<stdio.h>
int main() {
  double first, second, temp;
  printf("Enter first number: ");
  scanf("%lf", &first);
  printf("Enter second number: ");
  scanf("%lf", &second);
  temp = first;
  first = second;
  second = temp;
  printf(" first number = %.2lf\n", first);
  printf("second number = %.2lf", second);
  return 0;
}
